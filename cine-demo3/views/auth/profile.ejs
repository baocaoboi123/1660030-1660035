<section  style="background-color: #000;"   class="about10 py-3">
 
        <div class="row my-1 mx-auto">
        
          <div  class=" col-lg-12 col-md-12 col-12 w-50 pt-5 pb-5">                                   
            <div class="card-deck mx-auto">
              <div    class="img col-lg-3 col-md-3 col-12 w-50 pt-5 pb-5">
                                                 
                <img class="img-fluid container" src="<%- found_user.Pic_Description %>" alt="">      
                <div  class="container">

                    <form action="" method="post" class="form " enctype="multipart/form-data" >
                        <div class="form-group">
                            <label>Image Profile</label>
                            <br>
                            <input type="file" name="myImage">
                        </div>
                        <div class="form-group"> 
                            <button type="submit" class="btn">Submit</button>
                        </div>
                    </form>
                </div>
              </div> 
              <div  class="text col-lg-9 col-md-9 col-12 w-50 pt-5 pb-5">   
                    <div class="row">
                            <div class="row container d-flex">
                            <!-- <input placeholder="chon ngay" type="date" id="start1" > -->
              
                           
                                <div class="nav nav-pills ml-auto p-1">       
                                    <a href="/auth/profile/edit">Edit</a>
                                    </div>
                            </div>
                     </div>
                    <div class="row">
                                    <div  class="input-group mb-3">
                                        <div class="input-group-append">
                                          <span class="input-group-text" id="basic-addon2">Họ Tên</span>
                                        </div>
                                        <input readonly type="email" class="form-control" name="hoten" id="hoten" aria-describedby="emailHelp" value="<%- found_user.Hoten %>">

                                      </div>
                                      <div class="input-group mb-3">
                                            <div class="input-group-append">
                                              <span class="input-group-text" id="basic-addon2">Email</span>
                                            </div>
                                            <input readonly type="email" class="form-control" name="email" id="email" aria-describedby="emailHelp" value="<%- found_user.Email %>">
    
                                        </div>
                                        <div class="input-group mb-3">
                                          <div class="input-group-append">
                                            <span class="input-group-text" id="basic-addon2">Số điện thoại</span>
                                          </div>
                                          <input readonly type="email" class="form-control" name="sodienthoai" id="sodienthoai" aria-describedby="emailHelp" value="<%- found_user.Sodienthoai %>">
  
                                        </div>
                    </div>
                    
              </div>
             
            </div>
            
          </div>
      
          
        
        </div>
                  
      
      
</section>
      
            
      
      
      
      
      
      
      
      
      